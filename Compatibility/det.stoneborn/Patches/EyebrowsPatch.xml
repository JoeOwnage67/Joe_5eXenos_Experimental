<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    

    <!-- Joe_FA_NoBrows gene added to HillDwarf in favour of Det's Stoneborn Brows -->    
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/XenotypeDef[defName="Joe_HillDwarf_Xeno"]/genes</xpath>
        <value>
            <li>Joe_FA_NoBrows</li>
        </value>                                
    </Operation>

    <!-- Joe_FA_NoBrows gene added to MountainDwarf in favour of Det's Stoneborn Brows -->    
    <Operation Class="PatchOperationAdd">
        <xpath>/Defs/XenotypeDef[defName="Joe_MountainDwarf_Xeno"]/genes</xpath>
        <value>
            <li>Joe_FA_NoBrows</li>
        </value>                                
    </Operation>

    

</Patch>
    <xpath>Defs/FactionDef[@Name="OutlanderFactionBase"]/xenotypeSet/xenotypeChances</xpath>
    <nomatch Class="PatchOperationAdd">
      <xpath>Defs/FactionDef[@Name="OutlanderFactionBase"]</xpath>
      <value>
        <xenotypeSet>
          <xenotypeChances>
			<Baseliner>0.1</Baseliner>

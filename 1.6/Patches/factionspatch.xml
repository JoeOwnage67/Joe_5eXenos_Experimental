<?xml version="1.0" encoding="utf-8"?>

<!-- adding new xenos to existing factions -->
<Patch>
	
	<Operation Class="PatchOperationConditional">
    <xpath>Defs/FactionDef[@Name="OutlanderFactionBase" or @Name="TribeBase" or @Name="AncientsBase" or @Name="PirateBandBase"]/xenotypeSet/xenotypeChances</xpath>
		<nomatch Class="PatchOperationAdd">
			<xpath>Defs/FactionDef[@Name="OutlanderFactionBase" or @Name="TribeBase" or @Name="AncientsBase" or @Name="PirateBandBase"]</xpath>
			<value>
				<xenotypeSet>
					<xenotypeChances>
						<Joe_DarkElf_Xeno>0.1</Joe_DarkElf_Xeno>
						<Joe_Dragonborn_Xeno>0.1</Joe_Dragonborn_Xeno>
						<Joe_HillDwarf_Xeno>0.1</Joe_HillDwarf_Xeno>
						<Joe_Loxodon_Xeno>0.1</Joe_Loxodon_Xeno>
						<Joe_MountainDwarf_Xeno>0.1</Joe_MountainDwarf_Xeno>
						<Joe_Orc_Xeno>0.1</Joe_Orc_Xeno>
						<Joe_Shifter_Xeno>0.1</Joe_Shifter_Xeno>
						<Joe_Tabaxi_Xeno>0.1</Joe_Tabaxi_Xeno>
						<Joe_TorduneLoxo_Xeno>0.05</Joe_TorduneLoxo_Xeno>
						<Joe_Undead_Xeno>0.1</Joe_Undead_Xeno>
						<Joe_Warforged_Xeno>0.1</Joe_Warforged_Xeno>
						<Joe_WoodElf_Xeno>0.1</Joe_WoodElf_Xeno>							
					</xenotypeChances>
				</xenotypeSet>
			</value>
		</nomatch>
	</Operation>
	
	<Operation Class="PatchOperationConditional">
    <xpath>Defs/FactionDef[@Name="OutlanderFactionBase" or @Name="TribeBase" or @Name="AncientsBase" or @Name="PirateBandBase"]/xenotypeSet/xenotypeChances</xpath>
		<match Class="PatchOperationAdd">
			<xpath>Defs/FactionDef[@Name="OutlanderFactionBase" or @Name="TribeBase" or @Name="AncientsBase" or @Name="PirateBandBase"]/xenotypeSet/xenotypeChances</xpath>
			<value>
				<Joe_DarkElf_Xeno>0.1</Joe_DarkElf_Xeno>
				<Joe_Dragonborn_Xeno>0.1</Joe_Dragonborn_Xeno>
				<Joe_HillDwarf_Xeno>0.1</Joe_HillDwarf_Xeno>
				<Joe_Loxodon_Xeno>0.1</Joe_Loxodon_Xeno>
				<Joe_MountainDwarf_Xeno>0.1</Joe_MountainDwarf_Xeno>
				<Joe_Orc_Xeno>0.1</Joe_Orc_Xeno>
				<Joe_Shifter_Xeno>0.1</Joe_Shifter_Xeno>
				<Joe_Tabaxi_Xeno>0.1</Joe_Tabaxi_Xeno>
				<Joe_TorduneLoxo_Xeno>0.05</Joe_TorduneLoxo_Xeno>
				<Joe_Undead_Xeno>0.1</Joe_Undead_Xeno>
				<Joe_Warforged_Xeno>0.1</Joe_Warforged_Xeno>
				<Joe_WoodElf_Xeno>0.1</Joe_WoodElf_Xeno>			
			</value>
		</match>
	</Operation>
		

</Patch>
